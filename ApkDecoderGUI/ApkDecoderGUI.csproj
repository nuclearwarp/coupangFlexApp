<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net8.0-windows</TargetFramework>
    <UseWindowsForms>true</UseWindowsForms>
    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>

    <!-- 아이콘 파일을 쓸 경우 경로 유지, 아이콘 없이 먼저 빌드하고 싶으면 이 줄은 임시로 삭제/주석 처리 -->
    <ApplicationIcon>Assets\maruwell_flower.ico</ApplicationIcon>
  </PropertyGroup>

  <ItemGroup>
    <None Include="Assets\maruwell_flower.ico" />
    <None Include="Assets\maruwell_flower.png" />
  </ItemGroup>
</Project>
